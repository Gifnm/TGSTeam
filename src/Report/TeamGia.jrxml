     MamTrungBay."ViTri" AS MamTrungBay_ViTri,
     MamTrungBay."TrungBay" AS MamTrungBay_TrungBay
FROM
     "dbo"."SanPham" SanPham INNER JOIN "dbo"."MamTrungBay" MamTrungBay ON SanPham."Barcode" = MamTrungBay."Barcode"
