	<queryString>
		<![CDATA[SELECT
     SanPham."ID" AS SanPham_ID,
     SanPham."Barcode" AS SanPham_Barcode,
     SanPham."TenSP" AS SanPham_TenSP,
     SanPham."GiaMuon" AS SanPham_GiaMuon,
